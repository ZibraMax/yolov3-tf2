{
    // task,json CORRER PROGRAMAS PROS
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Convertir PASCAL a record",
            "command": "python",
            "args": [
                "tools/voc2012.py",
                "--data_dir",
                "./data/Output/ee",
                "--split",
                "train",
                "--classes",
                "./coco.names",
                "--output_file",
                "./data/dataset.record"
            ]
        },
        {
            "label": "Transfer learning de coco",
            "type": "shell",
            "command": "python",
            "args": [
                "train.py",
                "--dataset",
                "./data/dataset.record",
                "--val_dataset",
                "./data/dataset.record",
                "--classes",
                "./coco.names",
                "--mode",
                "fit",
                "--transfer",
                "fine_tune",
                "--weights",
                "./checkpoints/yolov3.tf",
                "--batch_size",
                "8",
                "--epochs",
                "10"
            ]
        },
        {
            "label": "Pasar de darknet a tf",
            "command": "python",
            "args": [
                "convert.py",
                "--weights",
                "./data/yolov3.weights",
                "--output",
                "./checkpoints/yolov3.tf"
            ]
        },
        {
            "label": "Correr analisis de imagen",
            "command": "python",
            "args": [
                "detect.py",
                "--weights",
                "./checkpoints/yolov3_train_5.tf",
                "--image",
                "./data/frames_cars/frame_1.png",
                "--output",
                "./prueba_frame2.png"
            ]
        },
        {
            "label": "Correr analisis de video",
            "command": "python",
            "args": [
                "detect_video.py",
                "--weights",
                "./checkpoints/yolov3_train_40.tf",
                "--video",
                "./data/videos_cars/test_chiqui.mp4",
                "--output",
                "./test_new_video.avi"
            ]
        }
    ]
}
